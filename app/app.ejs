<html lang="en" xmlns="http://www.w3.org/1999/xhtml">

<head>
    <meta charset="utf-8" http-equiv="Content-Security-Policy"
        content="script-src 'self' 'sha256-In6B8teKZQll5heMl9bS7CESTbGvuAt3VVV86BUQBDk='" />
    <title><%=APP_DISPLAY_NAME%></title>
    <script src="./assets/js/scripts/youtubeToAudio.js"></script>
    <script src="./assets/js/scripts/uicore.js"></script>
    <script src="./assets/js/scripts/uibinder.js"></script>
    <script src="./assets/js/scripts/skin3dview.js"></script>
    <link type="text/css" rel="stylesheet" href="./assets/css/launcher.css">

    <style>
        body {
            transition: background-image 1s ease;
            background-color: #8d4100;
            background-size: cover;
            -webkit-user-select: none;
        }

        /* G-07GDFHPTV5 */

        #main {
            display: none;
            height: calc(100% - 22px);
            background: linear-gradient(to top, rgba(0, 0, 0, 0.75) 0%, rgba(0, 0, 0, 0) 100%);
            width: 100%;
            position: absolute;
            z-index: 10;
        }

        #main[overlay] {
            filter: blur(3px) contrast(0.9) brightness(1.0);
        }
    </style>
</head>

<body bkid="<%=bkid%>" backgrounds="<%=backgrounds%>">
    <%- include('frame') %>
    <div id="main">
        <%- include('welcome') %>
        <%- include('login') %>
        <%- include('settings') %>
        <%- include('landing') %>
    </div>
    <%- include('overlay') %>
    <div id="loadingContainer">
        <div id="loadingContent">
            <div id="loadSpinnerContainer">
                <img class="bigToSmall" id="loadCenterImage" src="assets/images/LoadingSeal.png">
            </div>
        </div>
    </div>
    <script>
        // Load language
        for (let key of Object.keys(Lang.query('html'))) {
            document.getElementById(key).innerHTML = Lang.query(`html.${key}`)
        }
    </script>
</body>

</html>